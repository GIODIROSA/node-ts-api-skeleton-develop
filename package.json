{
  "name": "node-ts-api-skeleton",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/server.ts",
    "build": "npm run prisma:generate && tsc",
    "lint": "eslint . --ext .ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:rebuild": "docker-compose up -d --build",
    "docker:rebuild:clean": "docker-compose build --no-cache && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:api": "docker-compose logs -f api",
    "docker:logs:db": "docker-compose logs -f postgres",
    "docker:shell": "docker-compose exec api sh",
    "docker:db:shell": "docker-compose exec postgres psql -U postgres -d liferay_assets",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v",
    "docker:clean:containers": "docker-compose down",
    "docker:clean:all": "docker-compose down -v --rmi all"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@errors": "dist/errors",
    "@constants": "dist/v1/constants",
    "@utils": "dist/v1/utils",
    "@routes": "dist/v1/routes",
    "@services": "dist/v1/services",
    "@interfaces": "dist/v1/interfaces",
    "@middlewares": "dist/middlewares",
    "@validators": "dist/v1/validators",
    "@controllers": "dist/v1/controllers",
    "@libs": "dist/v1/libs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "prisma": "^6.19.0",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
